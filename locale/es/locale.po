msgstr "Este módulo permite hacer que los campos \"afiliación\" y \"ORCID\" sean obligatorios para los autores de un envío."

msgid "plugins.generic.toggleRequiredMetadata.settings.description"
msgstr "Seleccione los campos que serán obligatorios para los autores de un envío:"

msgid "plugins.generic.toggleRequiredMetadata.settings.requireOrcid"
msgstr "ORCID"

msgid "plugins.generic.toggleRequiredMetadata.settings.requireAffiliation"
msgstr "Afiliación"

msgid "plugins.generic.toggleRequiredMetadata.settings.requireBiography"
msgstr "Resumen Biográfico"

msgid "plugins.generic.toggleRequiredMetadata.settings.orcidProfilePluginEnabled"
msgstr "Funcionalidad no disponible porque que el módulo de perfil ORCID está activo."

msgid "plugins.generic.toggleRequiredMetadata.error"
msgstr "Por favor ingrese lo Resumen biográfico de su autor."


msgid "plugins.generic.toggleRequiredMetadata.stepValidation.error.orcid"
msgstr "El campo \"ORCID\" es obligatorio para todos los colaboradores"

msgid "plugins.generic.toggleRequiredMetadata.stepValidation.error.affiliation"
msgstr "El campo \"Afiliación\" es obligatorio para todos los colaboradores"

msgid "plugins.generic.toggleRequiredMetadata.stepValidation.error.biography"
msgstr "El campo \"Resumen biográfico\" es obligatorio para todos los colaboradores"